{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-z0-9_\\-]{1,255}$",
      "maxLength": 255
    },
    "data": {
      "type": "object"
    },
    "value": {
      "$ref": "./layerValue.json"
    },
    "parameter": {
      "type": "string"
    },
    "priority": {
      "type": "number"
    },
    "zoneId": {
      "type": "string",
      "pattern": "^[a-z0-9_\\-]{1,255}$",
      "maxLength": 255
    }
  },
  "required": [
    "id",
    "parameter",
    "priority",
    "zoneId"
  ]
}