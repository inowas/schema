{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "https://schema.inowas.com/",
    "title": "geometry",
    "description": "Geometry as defined by GeoJSON",
    "oneOf": [
        {
            "$ref": "geojson/linestring.json"
        },{
            "$ref": "geojson/point.json"
        },{
            "$ref": "geojson/polygon.json"
        }
    ]
}
