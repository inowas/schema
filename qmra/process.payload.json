{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "object": {
    "properties": {
      "TreatmentID": {
        "type": "number"
      },
      "TreatmentName": {
        "type": "string"
      },
      "TreatmentGroup": {
        "type": "string"
      },
      "PathogenGroup": {
        "type": "string"
      },
      "type": {
        "enum": ["log10_norm", "uniform"]
      },
      "min": {
        "type": "number"
      },
      "max": {
        "type": "number"
      }
    },
    "required": ["TreatmentID", "TreatmentName", "TreatmentGroup", "PathogenGroup", "type", "min", "max"]
  }
}