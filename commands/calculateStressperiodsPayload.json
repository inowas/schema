{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
        },
        "start": {
            "type": "string",
            "minLength": 3,
            "maxLength": 255
        },
        "end": {
            "type": "string"
        },
        "initial_steady": {
            "type": "boolean"
        }
    },
    "required": [
        "id",
        "start",
        "end"
    ]
}
